
<%= current_user.email %>

<%= form_for [@listing, Reservation.new] do |t| %>

	
<br/>
	
<br/>

<div class="container">
    <div class='col-md-5'>
        <div class="form-group">
            <%= t.label :date_in %>
            <div class='input-group date' id='datetimepicker6'>
                <input class="form-control" name="reservation[date_in]" >

                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>
    <div class='col-md-5'>
        <div class="form-group">
            <%= t.label :date_out %>
            <div class='input-group date' id='datetimepicker7'>
                <input class="form-control" name="reservation[date_out]">
                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>
</div>

<div class="demo-frame">



	
	  
	
<br/>



<%= t.submit %>

<% end %>